<% include partials/header.ejs %>
<div class="containter text-center">
<% list.forEach(function(item){ %>
<div class="favoriteItem">
<ul>
	<li class="favoriteListItem col-md-4"><a href="/info/<%=item.movieId%>"><%=item.title%></a></li>
</ul>
<div class="deleteButton">
	<a href="#" type="submit" class="delete btn btn-danger" data-id="<%= item.id %>"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></a>
</div>
</div>
<% }) %>
</div>
<% include partials/footer.ejs %>